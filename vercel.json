{
  "version": 2,
  "routes": [
    {
      "src": "/_nuxt/(.*)",
      "headers": { "cache-control": "public,max-age=31536000,immutable" },
      "continue": true
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|gif|svg|ico)$)",
      "headers": { "cache-control": "public,max-age=31536000,immutable" },
      "continue": true
    }
  ]
}
