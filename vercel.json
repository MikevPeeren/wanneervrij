{
  "version": 2,
  "routes": [
    {
      "src": "/_nuxt/(.*)",
      "headers": { "cache-control": "public,max-age=31536000,immutable" },
      "continue": true
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|gif|svg|ico)$)",
      "headers": { "cache-control": "public,max-age=31536000,immutable" },
      "continue": true
    },
    {
      "src": "/(.*)",
      "headers": { "cache-control": "s-maxage=1, stale-while-revalidate" },
      "continue": true
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "wanneervrij.nl"
        }
      ],
      "destination": "https://www.wanneervrij.nl/:path*",
      "permanent": true
    }
  ]
}
